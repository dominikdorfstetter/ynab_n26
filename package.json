{
  "name": "ynab_n26",
  "version": "0.0.1",
  "description": "converts CSV output from n26 Bank into CSV readable by YNAB (YouNeedABudget)",
  "main": "index.js",
  "repository": "git@github.com:dominikdorfstetter/ynab_n26.git",
  "author": "Dominik Dorfstetter <dorfstetter@posteo.de>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "node --inspect=5858 -r ts-node/register ./src/parser.ts",
    "start:watch": "nodemon",
    "build": "tsc"
  },
  "dependencies": {
    "csv-parser": "^2.3.0",
    "minimist": "^1.2.0",
    "typescript": "^3.6.2"
  },
  "devDependencies": {
    "@types/node": "^12.7.4",
    "eslint": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "nodemon": "^1.19.2",
    "ts-node": "^8.3.0"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}
